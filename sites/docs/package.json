{
    "name": "docs",
    "version": "0.0.6",
    "private": true,
    "sideEffects": false,
    "description": "Doc of Svxui library",
    "homepage": "https://github.com/radiium/svxui",
    "license": "MIT",
    "author": {
        "name": "radiium",
        "email": "remip.dev@gmail.com",
        "url": "https://github.com/radiium"
    },
    "keywords": [
        "svelte",
        "sveltekit",
        "ui",
        "library",
        "ui-components",
        "components",
        "components-library",
        "open-source",
        "typescript",
        "web",
        "monorepo"
    ],
    "bugs": {
        "url": "https://github.com/radiium/svxui/issues"
    },
    "type": "module",
    "engines": {
        "node": ">=18",
        "pnpm": ">=9"
    },
    "scripts": {
        "dev": "pnpm run /^dev:.*/",
        "dev:content": "contentlayer dev",
        "dev:svelte": "vite dev --host",
        "build": "pnpm run /^build:.*/",
        "build:content": "contentlayer build",
        "build:svelte": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier ./eslint.config.js --ignore-path=../../.prettierignore --check . && eslint .",
        "format": "prettier ./eslint.config.js --ignore-path=../../.prettierignore --write . --cache && eslint --fix .",
        "sync": "svelte-kit sync"
    },
    "dependencies": {
        "@speed-highlight/core": "^1.2.6",
        "phosphor-svelte": "^2.0.1",
        "svxui": "workspace:*"
    },
    "devDependencies": {
        "@configs/eslint-config": "workspace:*",
        "@configs/prettier-config": "workspace:*",
        "@sveltejs/adapter-auto": "^3.3.0",
        "@sveltejs/adapter-vercel": "^5.4.5",
        "@sveltejs/kit": "^2.7.2",
        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "concurrently": "^9.0.1",
        "contentlayer": "^0.3.4",
        "date-fns": "^4.1.0",
        "eslint": "^9.13.0",
        "mdsx": "^0.0.6",
        "next-contentlayer": "^0.3.4",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
        "rehype-pretty-code": "^0.14.0",
        "rehype-slug": "^6.0.0",
        "rehype-toc": "^3.0.2",
        "remark-capitalize": "^1.1.0",
        "remark-gfm": "^4.0.0",
        "shiki": "^1.22.1",
        "svelte": "^4.2.19",
        "svelte-check": "^4.0.5",
        "typescript": "^5.6.3",
        "unified": "^11.0.5",
        "unist-util-visit": "^5.0.0",
        "vite": "^5.4.10"
    }
}